{
	"name": "Chrome Image Uncenterer",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Un-centers the image viewer introduced in Chrome 56.0+",
	"homepage_url": "http://github.com/i-a-n/chrome-image-uncenterer",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [{
		"matches": [
			"*://*/*.jpg*",
			"*://*/*.jpeg*",
			"*://*/*.gif*",
			"*://*/*.svg*",
			"*://*/*.png*",
			"*://*/*.tiff*",
			"*://*/*.bmp*"
		],
		"css": [
			"src/inject/inject.css"
		],
		"run_at": "document_end"
	}]
}
